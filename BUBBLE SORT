//BUBBLE SORT-REPEATEDLY SWAP THE ADJACENT ELEMENTS IN WRONG ORDER
#include <iostream>
using namespace std;

int main() {
    int arr[]={8,45,23,19,51,12};
	int num=sizeof(arr)/sizeof(arr[0]);
	
	for(int i=0;i<num;i++)
	{
	    for(int j=0;j<num;j++)
	    {
	        if(arr[i]<arr[j])
	        swap(arr[i],arr[j]);
	    }
	}
	for(int i=0;i<num;i++)
	{
	    cout<<" "<<arr[i]<<" ";
	}
	return 0;
}
